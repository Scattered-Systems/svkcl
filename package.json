    "build:sidebar": "npm run build --workspace=@svkcl/sidebar",
    "build:table": "npm run build --workspace=@svkcl/table",
    "build:text": "npm run build --workspace=@svkcl/text",
    "build:tag": "npm run build --workspace=@svkcl/tag",
